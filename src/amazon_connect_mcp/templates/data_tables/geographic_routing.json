      "action": "Evaluate",
      "primaryAttribute": "area_code",
      "primaryValue": "$.CustomerEndpoint.Address.substring(2,5)",
      "returnAttributes": ["primary_queue", "timezone", "language", "regional_hours_start", "regional_hours_end"]
    }
  }
