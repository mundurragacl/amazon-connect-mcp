{
  "name": "Geographic Routing Rules",
  "description": "Location-based routing for regional support centers",
  "attributes": [
    {"name": "area_code", "type": "text", "isPrimaryKey": true, "description": "Phone area code or region code"},
    {"name": "region_name", "type": "text", "required": true, "description": "Region display name"},
    {"name": "timezone", "type": "text", "required": true, "description": "IANA timezone"},
    {"name": "primary_queue", "type": "text", "required": true, "description": "Primary regional queue"},
    {"name": "language", "type": "text", "default": "en-US", "description": "Primary language"},
    {"name": "secondary_language", "type": "text", "description": "Secondary language if bilingual"},
    {"name": "regional_hours_start", "type": "text", "description": "Regional hours start (HH:MM local)"},
    {"name": "regional_hours_end", "type": "text", "description": "Regional hours end (HH:MM local)"},
    {"name": "overflow_region", "type": "text", "description": "Overflow to another region after hours"}
  ],
