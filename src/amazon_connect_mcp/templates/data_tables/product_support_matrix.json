{
  "name": "Product Support Matrix",
  "description": "Product-specific support routing and information for technology companies",
  "industry": "Technology/SaaS",
  "attributes": [
    {"name": "product_code", "type": "text", "isPrimaryKey": true, "description": "Product identifier"},
    {"name": "product_name", "type": "text", "required": true, "description": "Product display name"},
    {"name": "support_queue", "type": "text", "required": true, "description": "Primary support queue"},
    {"name": "tier2_queue", "type": "text", "description": "Tier 2 escalation queue"},
    {"name": "support_level", "type": "text", "description": "Support level: basic, standard, premium, enterprise"},
    {"name": "knowledge_base_id", "type": "text", "description": "Associated knowledge base"},
    {"name": "current_version", "type": "text", "description": "Current product version"},
    {"name": "eol_date", "type": "text", "description": "End of life date if applicable"},
    {"name": "known_issues_url", "type": "text", "description": "URL to known issues page"},
    {"name": "remote_support_enabled", "type": "boolean", "default": true, "description": "Remote session support available"}
  ],
  "sampleData": [
    {
