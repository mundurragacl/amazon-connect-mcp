  "flowUsage": {
    "description": "Route support calls based on product",
    "example": {
      "action": "Evaluate",
      "primaryAttribute": "product_code",
      "primaryValue": "$.Attributes.ProductCode",
      "returnAttributes": ["support_queue", "support_level", "knowledge_base_id", "current_version"]
    }
  }
}
