{
  "name": "Healthcare Provider Directory",
  "description": "Provider and department routing for healthcare contact centers",
  "industry": "Healthcare",
  "attributes": [
    {"name": "department_code", "type": "text", "isPrimaryKey": true, "description": "Department or specialty code"},
    {"name": "department_name", "type": "text", "required": true, "description": "Department display name"},
    {"name": "queue_name", "type": "text", "required": true, "description": "Target queue"},
    {"name": "transfer_number", "type": "text", "description": "Direct transfer number if applicable"},
    {"name": "hours_start", "type": "text", "description": "Operating hours start (HH:MM)"},
    {"name": "hours_end", "type": "text", "description": "Operating hours end (HH:MM)"},
    {"name": "after_hours_action", "type": "text", "description": "After hours: voicemail, answering_service, emergency"},
    {"name": "appointment_scheduling", "type": "boolean", "default": true, "description": "Supports appointment scheduling"},
    {"name": "urgent_line_available", "type": "boolean", "default": false, "description": "Has urgent/emergency line"}
  ],
